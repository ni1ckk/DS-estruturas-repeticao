//Faça um algoritmo que receba um número e mostre uma mensagem caso este número seja maior que 10
programa {
  funcao inicio() {
    
    inteiro a, b, c

    faca {
      escreva("Digite um número: ")
      leia(a)
    }
    enquanto (a <10)
    escreva("\nO número digitado é maior ou igual a 10")
  }
}